<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Rock Paper Scissors"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Rules" />
        <activity android:name=".Play" />
        <activity android:name=".Level" />
        <activity android:name=".Play10" />
        <activity android:name=".Play15" />
        <activity android:name=".Play20" />
        <activity android:name=".Play30"></activity>
    </application>

</manifest>